// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "out": false
    },
    "search.exclude": {
        "out": true
    },
    "typescript.tsdk": "./node_modules/typescript/lib",
    "cSpell.words": [
        "repr",
        "Takashi"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll": "always",
        "source.organizeImports": "always"
    }
}