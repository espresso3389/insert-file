{
  "name": "insert-file",
  "displayName": "insert-file",
  "description": "Insert file on the current text editor in plain/base64/uuencode.",
  "version": "0.0.1",
  "publisher": "espresso3389",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Sushi"
  ],
  "icon": "file.png",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/espresso3389/insert-file/issues"
  },
  "homepage": "https://github.com/espresso3389/insert-file",
  "repository": {
    "type": "git",
    "url": "https://github.com/espresso3389/insert-file.git"
  },
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [{
      "command": "extension.sayHello",
      "title": "Hello World"
    }]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.10",
    "vscode": "^1.0.3",
    "mocha": "^3.2.0",
    "@types/node": "^0.0.2",
    "@types/mocha": "^2.2.33"
  }
}